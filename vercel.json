{
    "version": 2,
    "builds": [
      {
        "src": "index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ],
    "env": {
      "MY_ENV_VAR": "value"
    },
    "public": "./public",
    "regions": ["all"]
  }
  